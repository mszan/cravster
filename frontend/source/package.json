{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "generate": "openapi --input http://cravster-backend:3000/docs-json --output ./src/client/generated --request ./src/client/request.ts",
        "format": "prettier --write src",
        "watch": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@devexpress/dx-react-chart": "4.0.5",
        "@emotion/react": "11.11.1",
        "@emotion/styled": "11.11.0",
        "@fontsource/roboto": "5.0.5",
        "@tanstack/query-sync-storage-persister": "^4.33.0",
        "@tanstack/react-query": "^4.33.0",
        "@tanstack/react-query-persist-client": "^4.33.0",
        "@tinymce/tinymce-react": "^4.3.0",
        "@types/jest": "29.5.3",
        "@types/node": "20.4.5",
        "@types/react": "18.2.17",
        "@types/react-dom": "18.2.7",
        "antd": "^5.20.5",
        "axios": "1.4.0",
        "date-fns": "2.30.0",
        "framer-motion": "^10.16.3",
        "html-react-parser": "^4.2.0",
        "isbot": "^3.7.1",
        "jsonwebtoken": "9.0.1",
        "jwt-decode": "3.1.2",
        "lodash": "4.17.21",
        "react": "18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-countup": "^6.5.3",
        "react-dom": "18.2.0",
        "react-helmet": "^6.1.0",
        "react-highlight-words": "^0.20.0",
        "react-in-viewport": "^1.0.0-alpha.30",
        "react-router-dom": "6.14.2",
        "react-scroll-parallax": "^3.4.4",
        "react-validation": "3.0.7",
        "sass": "^1.64.2",
        "store": "2.0.12",
        "typescript": "4.9.5",
        "usehooks-ts": "^2.9.1",
        "web-vitals": "3.4.0",
        "yup": "1.2.0"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "9.0.2",
        "@types/lodash": "4.14.196",
        "@types/react-beautiful-dnd": "^13.1.4",
        "@types/react-helmet": "^6.1.9",
        "@types/react-highlight-words": "^0.20.0",
        "@types/store": "2.0.2",
        "eslint-config-prettier": "8.9.0",
        "eslint-plugin-prettier": "5.0.0",
        "openapi-typescript-codegen": "0.25.0",
        "prettier": "3.0.0",
        "react-scripts": "5.0.1"
    },
    "prettier": {
        "printWidth": 140,
        "tabWidth": 4,
        "trailingComma": "all",
        "arrowParens": "avoid",
        "singleQuote": false
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "prettier"
        ],
        "plugins": [
            "@typescript-eslint/eslint-plugin",
            "prettier"
        ],
        "ignorePatterns": [
            "/node_modules",
            "/build",
            "/temp"
        ],
        "rules": {
            "@typescript-eslint/no-inferrable-types": "off",
            "semi": "warn",
            "quotes": [
                "warn",
                "double",
                {
                    "allowTemplateLiterals": true
                }
            ],
            "prettier/prettier": "warn"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
